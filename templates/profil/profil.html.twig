{% extends 'base.html.twig' %}

{% block title %} {{ parent() }} |Mon profil{% endblock %}

{% block h1 %}{% endblock %}

{% block body %}
<ul class="list-group col-md-6 mx-auto">
  <li class="list-group-item">Email :  {{app.user.email}}  </li>
  <li class="list-group-item">Nom  :  {{app.user.nom|upper }} </li>
  <li class="list-group-item">Prenpm  :  {{app.user.prenom|capitalize}} </li>
  <li class="list-group-item">Date d'inscription  :  {{app.user.createAt|date('d/m/y') }} </li>

</ul>
{% endblock %}
